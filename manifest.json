{
  "name": "__MSG_appName__",
  "version": "0.0.3",
  "manifest_version": 2,
  "description": "__MSG_appDescription__",
  "icons": {
    "48": "images/icon-48.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
      "scripts/chromereload.js",
      "scripts/background.js"
    ]
  },
  "browser_action": {
    "default_icon": {
      "48": "images/icon-48.png"
    },
    "default_title": "own-your-history",
    "default_popup": "popup.html"
  },
  "options_page": "options.html",
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "jquery-1.11.2.min.js",
        "scripts/contentscript.js"
      ],
      "run_at": "document_end",
      "all_frames": false
    }
  ],
  "permissions": [
    "tabs",
    "history",
    "http://*/*",
    "https://*/*",
    "storage"
  ]
}
